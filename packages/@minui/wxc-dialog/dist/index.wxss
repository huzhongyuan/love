.dialog {
    background: #ffffff;
    width: 676rpx;
    border-radius: 8rpx;
    background-color: #ffffff;
    display: flex;
    flex-flow: column nowrap;
    align-items: center;
}
.no {
  width: 100%;
  display: flex;
  justify-content: flex-end;
  margin-right: 14rpx;
}
.dialog__title {
    font-size: 32rpx;
    margin-bottom: 18rpx;
    padding: 0 36rpx;
    color: #333333;
    display: flex;
    flex-direction: row;
    align-items: center;
}
.line{
  width: 40rpx;
  height: 2rpx;
  background-color: #333333;
  margin: 0 35rpx;
}
.dialog__content{
  width: 594rpx;
  height: 650rpx;
  overflow: scroll;
  margin: 0 41rpx 81rpx 41rpx;
  display: flex;
  justify-content: flex-start;
  flex-direction: column;
}
.rangeTitle {
  margin-top: 81rpx;
  font-size: 30rpx;
  color: #333333;
}
.rangeContent {
  margin-top: 29rpx;
  color: #333333;
  opacity: 0.7;
  font-size: 22rpx;
}

.dialog__btns {
    width: 676rpx;
    height: 100rpx;
    border-top: 1rpx solid #e5e5e5;
    display: flex;
    flex-direction: row;
    justify-content: center;
    align-items: center;
    font-size: 36rpx
}
.dialog__btns--confirm image {
  width: 196rpx;
  height: 82rpx;
}
.black {
  display: flex;
    width: 200rpx;
    height: 10rpx;
}
.dialog__btns--confirm {
  display: flex;
}
.dialog__btns--cancel {
  display: flex;
  width: 200rpx;
  height: 80rpx;
  justify-content: center;
  font-size: 26rpx;
  color: #333333;
  align-items: center;
}